pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                // Checkout your Java code repository
                // Replace the repository URL below with your actual repository URL
                git 'https://github.com/Nagavamshia/Jenkins-Zero-To-Hero/myprgm.git'
            }
        }

        stage('Build') {
            steps {
                // Compile the Java code
                sh 'javac HelloWorld.java'
            }
        }

        stage('Run') {
            steps {
                // Execute the compiled Java program
                sh 'java HelloWorld'
            }
        }
    }
}
